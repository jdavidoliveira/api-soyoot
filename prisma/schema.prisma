generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id @default(uuid())
  email     String @unique
  senha     String
  nome      String
  sobrenome String

  @@map("user")
}

model Empresa {
  id       String @id @default(uuid())
  telefone String
  email    String @unique
  cnpj     String @unique
  nome     String
}
